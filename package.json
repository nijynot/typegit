{
  "name": "diary",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "update-schema": "gulp babel-schema & node ./scripts/updateSchema.js",
    "relay-compile": "relay-compiler --src ./views/ --schema ./graph/schema.graphql",
    "build": "npm run update-schema && npm run relay-compile",
    "sass": "gulp watch",
    "webpack:dev": "cross-env NODE_ENV=development webpack --progress --watch",
    "start:dev": "nodemon server.js",
    "dump": "mysqldump --databases to_the_archives > ./sql/to_the_archives.sql -u root -p",
    "dump:nodata": "mysqldump --databases --no-data to_the_archives > ./sql/to_the_archives-nodata.sql -u root -p"
  },
  "dependencies": {
    "autosize": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "date-fns": "^1.29.0",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "express-session": "^1.15.6",
    "fs-extra": "^4.0.2",
    "graphql": "^0.11.7",
    "lodash": "^4.17.4",
    "markdown-it": "^8.4.0",
    "markdown-it-sup": "^1.0.0",
    "moment": "^2.19.1",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.6.0",
    "randexp": "^0.4.6",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-relay": "^1.4.1",
    "react-textarea-autosize": "^5.1.0",
    "redis": "^2.8.0",
    "relay-runtime": "^1.4.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-relay": "^1.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^3.1.0",
    "gulp-watch": "^4.3.11",
    "json-loader": "^0.5.7",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "relay-compiler": "^1.4.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1"
  }
