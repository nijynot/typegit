    "fs-extra": "^4.0.2",
    "graphql": "^0.11.7",
    "graphql-base64": "^1.0.1",
    "graphql-connection": "^1.1.0",
    "graphql-relay": "^0.5.4",
    "immutability-helper": "^2.5.0",
    "linkify-it": "^2.0.3",
