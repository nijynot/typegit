{
  "name": "diary",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest lib",
    "schema": "gulp babel-config && gulp babel-schema && node ./scripts/updateSchema.js",
    "relay-compile": "relay-compiler --src ./src/views/ --schema ./graph/schema.graphql",
    "build": "npm run schema && npm run relay-compile -- --watch",
    "sass": "gulp sass & gulp watch",
    "webpack.dev": "cross-env NODE_ENV=development webpack --progress --watch",
    "webpack.prod": "cross-env NODE_ENV=production webpack --progress --define process.env.NODE_ENV=\"'production'\"",
    "start.dev": "nodemon server.js",
    "start.prod": "cross-env NODE_ENV=production nodemon server.js",
    "start.test": "cross-env NODE_ENV=test node server.js",
    "deamon.dev": "pm2 start server.js",
    "deamon.prod": "cross-env NODE_ENV=production pm2 start server.js",
    "deamon.prod.restart": "cross-env NODE_ENV=production pm2 restart server.js",
    "dump.data": "mysqldump --databases diary > ./backup/backup.sql -u root -p",
    "dump.structure": "mysqldump --databases --no-data diary > ./sql/structure.sql -u root -p",
    "dump.test.data": "mysqldump --databases test_diary > ./backup/test_backup.sql -u root -p",
    "dump.test.structure": "mysqldump --databases --no-data test_diary > ./sql/test_structure.sql -u root -p"
  },
  "dependencies": {
    "autosize": "^4.0.0",
